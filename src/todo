(x, y) -> tile index

(32, 32) -> 0
(0, 0) -> 0
(16, 0) -> 0

(33, 0) -> 1
(0, 33) -> tiles_x

x = x / tileWidth - 1;
y = y / tileHeight - 1;

index = y * width + x
